        "def get_word_count(text):\n",
        "    word = text.split()\n",
        "    return len(word)\n",
        "\n",
        "df['Word Count'] = df['Review Text'].apply(get_word_count)"
      ],
      "metadata": {
        "id": "qwXH7FjmVcwU"
      },
      "execution_count": 37,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def get_bigrams(text):\n",
        "    wordd = nltk.word_tokenize(text.lower())\n",
        "    bigram_list = list(ngrams(wordd, 2))\n",
        "    return [' '.join(bigram) for bigram in bigram_list]\n",
        "\n",
        "df['Bigrams'] = df['Review Text'].apply(get_bigrams)"
      ],
      "metadata": {
        "id": "h0KitoM6OxQS"
      },
      "execution_count": 38,
      "outputs": []
    },
